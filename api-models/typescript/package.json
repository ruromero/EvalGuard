{
  "name": "@trustification/evalguard-api-model",
  "version": "1.0.0-SNAPSHOT",
  "description": "TypeScript API client for EvalGuard",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "generate": "node scripts/generate.js",
    "dev": "ts-node src/index.ts",
